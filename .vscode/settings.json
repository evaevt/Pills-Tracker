{
    "sourcekit-lsp.serverPath": "/usr/bin/xcrun",
    "sourcekit-lsp.serverArguments": [
        "sourcekit-lsp"
    ],
    "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
    "swift.buildPath": "/usr/bin/xcrun",
    "swift.buildArguments": [
        "swift",
        "build"
    ],
    "files.associations": {
        "*.swift": "swift"
    },
    "editor.formatOnSave": true,
    "swift.format.enable": true,
    "sweetpad.buildServer.enabled": true,
    "sweetpad.buildServer.path": "/opt/homebrew/bin/xcode-build-server",
    "sweetpad.xcbeautify.enabled": true,
    "sweetpad.xcbeautify.path": "/opt/homebrew/bin/xcbeautify",
    "swift.swiftEnvironmentVariables": {
        "DEVELOPER_DIR": "/Applications/Xcode.app"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".yoyo": true
    },
    "sweetpad.build.xcodeWorkspacePath": "Tracker iOS.xcodeproj.backup/project.xcworkspace"
}